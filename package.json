{
  "name": "strandersson.se",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "elm-test \"src/**/*Tests.elm\"",
    "test:watch": "elm-test \"src/**/*Tests.elm\" --watch",
    "start": "BUILD_TAG=$(date '+%Y%m%d-%H%M%S') parcel ",
    "build": "BUILD_TAG=$(date '+%Y%m%d-%H%M%S') parcel build --no-source-maps",
    "docker:build": "BUILD_TAG=$(date '+%Y%m%d-%H%M%S') docker build --build-arg=BUILD_TAG -t strandersson .",
    "docker:start": "docker run --rm -it -p 80:80 strandersson",
    "deploy:latest": "./scripts/digitalocean.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johan-st/strandersson.se.git"
  },
