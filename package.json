{
  "name": "strandersson.se",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "elm-test \"src/**/*Tests.elm\"",
    "test:watch": "elm-test \"src/**/*Tests.elm\" --watch",
    "start": "set BUILD_TAG=%DATE% %TIME% && parcel src/index.html && set BUILD_TAG= ",
    "build": "set BUILD_TAG=%DATE% %TIME% && parcel build src/index.html && set BUILD_TAG=",
    "build:noEnv": "parcel build src/index.html",
    "docker:build": "set BUILD_TAG=%DATE% %TIME% && docker build --build-arg=BUILD_TAG -t johanst/strandersson.se  ../  && set BUILD_TIME=",
    "docker:run": "docker run --rm -it -p 80:80 johanst/strandersson.se",
    "report": "parcel build src/index.html --reporter @parcel/reporter-bundle-analyzer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johan-st/strandersson.se.git"
